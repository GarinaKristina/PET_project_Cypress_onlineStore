context('Boden.com online store', () => {
  it('should sign in on account', () => {
    cy.visit('https://www.boden.co.uk');
    cy.get('button[aria-label="Burger Menu Icon"]').click();
    cy.get('div[class="subContainer parsys header-rd__after-nav-wrapper"] a:nth-child(1)').click();
    cy.get('#email').type('bodencustomer@gmail.com');
    cy.get('#password').type('Qwerty789456123!');
    cy.get('#login').click();
    cy.get('button[aria-label="Burger Menu Icon"]').click();
  });
});
